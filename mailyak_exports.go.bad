package main

import (
	"reflect"

	"github.com/domodwyer/mailyak"
)

func init() {
	GotxSymbols["github.com/domodwyer/mailyak"] = map[string]reflect.Value{
		// function, constant and variable definitions
		"New": reflect.ValueOf(mailyak.New),

		// type definitions
		"BodyPart": reflect.ValueOf((*mailyak.BodyPart)(nil)),
		"MailYak":  reflect.ValueOf((*mailyak.MailYak)(nil)),
	}
}
